## -*- conf -*-

"vol up"
  XF86AudioRaiseVolume

"vol down"
  XF86AudioLowerVolume

"vol toggle"
  XF86AudioMute

## lock screen
# "slock"
"xscreensaver-command -lock"
  Scroll_Lock

## clipboard manager https://github.com/cdown/clipmenu/
"clipmenu -fn 'Roboto Mono-12'"
  Mod4 + y

## push primary to clipboard, for use by broken gtk apps
"xsel -p | xsel -b -i"
  Mod4 + Shift + y

## run commands
"rofi -show run"
  Mod4 + r

"emacsclient -c"
  Mod4 + equal

"rofi -show run"
  Mod4 + space

## list windows
"rofi -show window"
  Mod4 + w

## pass
# "passmenu -fn 'Terminus:style=Regular:size=12'"
"passdmenu"
  Mod4 + u

## pass username
"LINE=2 passmenu -fn 'Terminus:style=Regular:size=12'"
  Mod4 + Shift + u

## enable trackpad
"xinput enable 11"
  Mod4 + Delete

## disable trackpad
"xinput disable 11"
  Mod4 + BackSpace

"setxkbmap -option ctrl:swapcaps"
  Mod4 + Caps_Lock

"xfce4-screenshooter"
  Print

"flameshot&; flameshot gui -r | xclip -selection clipboard -t image/png; pkill flameshot"
  Mod4 + Print

"setxkbmap -option ctrl:swapcaps"
  Mod4 + z

"xkill"
  Mod4 + x

# "dunstctl close"
#   Mod4 + minus

# "dunstctl history-pop"
#   Mod4 + backslash

  Pause
