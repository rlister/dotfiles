Emacs.font: Roboto Mono:size=15

!! turn off nonsense
Emacs.cursorBlink: false
Emacs.menuBar: false
Emacs.horizontalScrollBars: false
Emacs.verticalScrollBars: false
Emacs.toolBar: false

Emacs.background: Black
!Emacs.foreground: azure4
!Emacs.cursorColor: PaleGreen

!! standard faces https://www.gnu.org/software/emacs/manual/html_node/emacs/Standard-Faces.html
Emacs.default.attributeForeground: azure4
Emacs.italic.attributeUnderline: false
Emacs.italic.attributeFamily: Roboto Mono-Italic
Emacs.underline.attributeUnderline: false
Emacs.fixed-pitch.attributeFamily: Roboto Mono
Emacs.fixed-pitch-serif.attributeFamily: Roboto Mono
Emacs.variable-pitch.attributeFamily: Roboto Mono
Emacs.shadow.attributeForeground: DarkSlateGray4

!! highlights
Emacs.highlight.attributeBackground: grey10
Emacs.isearch.attributeForeground: PaleGreen
Emacs.isearch.attributeBackground: Black
Emacs.isearch-fail.attributeForeground: IndianRed
Emacs.isearch-fail.attributeBackground: Black
Emacs.query-replace.attributeForeground: PaleGreen
Emacs.lazy-highlight.attributeBackground: grey10
Emacs.lazy-highlight.attributeForeground: DarkGoldenrod
Emacs.region.attributeBackground: MidnightBlue
Emacs.secondary-selection.attributeBackground: grey15
Emacs.trailing-whitespace.attributeBackground: IndianRed4

!! mode-line
Emacs.mode-line.attributeForeground: aquamarine3
Emacs.mode-line.attributeBackground: #222222
Emacs.mode-line.attributeBox: nil
Emacs.mode-line-inactive.attributeForeground: #857b6f
Emacs.mode-line-inactive.attributeBackground: #111111
Emacs.mode-line-inactive.attributeBox: nil
Emacs.mode-line-inactive.attributeWeight: normal
Emacs.mode-line-buffer-id.attributeForeground: PaleGreen
Emacs.mode-line-highlight.attributeBox: nil
Emacs.mode-line-highlight.attributeForeground: Cyan

!! other parts of frame
Emacs.header-line.attributeForeground: MediumAquamarine
Emacs.header-line.attributeBackground: grey10
Emacs.minibuffer-prompt.attributeForeground: PaleGreen
Emacs.fringe.attributeBackground: Black
Emacs.cursor.attributeBackground: PaleGreen

Emacs.link.attributeForeground: CornflowerBlue
Emacs.link.attributeUnderline: false

!! inherit from these
Emacs.success.attributeForeground: PaleGreen
Emacs.warning.attributeForeground: PaleGoldenrod
Emacs.error.attributeForeground: IndianRed
Emacs.match.attributeForeground: RosyBrown

!! needed to render menus
!XTerm*font: -misc-fixed-medium-r-semicondensed--0-0-75-75-c-0-iso8859-1
XTerm*font: -*-terminus-medium-*-*-*-18-*-*-*-*-*-iso10646-1

!! default to truetype font
xTerm.VT100.renderFont: true

! ! XTerm.vt100.faceName: xos4 Terminus:size=14:antialias=false
XTerm*faceName: Roboto Mono:size=12:antialias=false
! ! VT Font Menu: Unreadable
xterm*faceSize1: 8
! ! VT font menu: Tiny
xterm*faceSize2: 10
! ! VT font menu: Medium
xterm*faceSize3: 12
! ! VT font menu: Large
xterm*faceSize4: 16
! ! VT font menu: Huge
xterm*faceSize5: 22



XTerm*foreground:  	White
XTerm*cursorColor: 	PaleGreen
XTerm.vt100.background:	Black
XTerm.vt100.foreground:	azure4
XTerm*vt100*highlightColor: MidnightBlue
XTerm.vt100.locale: true

!! menu colors
XTerm*mainMenu*background: DodgerBlue4
XTerm*vtMenu*background:   DodgerBlue4
XTerm*fontMenu*background: DodgerBlue4

!! use alt as in emacs instead of 8bit chars
XTerm.vt100.metaSendsEscape: true

!! default scrollback is 1024
XTerm.vt100.saveLines: 4096

!! just no
XTerm*ScrollBar: off
XTerm*vt100*rightScrollBar: True
XTerm*Scrollbar*foreground: DodgerBlue4
XTerm*Scrollbar*background: Black

!! ansi colors
XTerm*colorMode:    True
XTerm*dynamicColor: On

XTerm*VT100*color0:  Black
XTerm*VT100*color1:  IndianRed
XTerm*VT100*color2:  PaleGreen
XTerm*VT100*color3:  DarkGoldenrod
XTerm*VT100*color4:  #B5A0FF
XTerm*VT100*color5:  Gray
XTerm*VT100*color6:  CornflowerBlue
XTerm*VT100*color7:  White
XTerm*VT100*color8:  Black

xscreensaver.passwd.passwdFont:         -*-terminus-medium-r-*-*-18-*-*-*-*-*-*-*
! XTerm*VT100*color9:  Red
! XTerm*VT100*color10: Green
! XTerm*VT100*color11: Yellow
! XTerm*VT100*color12: #B5A0FF
! XTerm*VT100*color13: Gray
! XTerm*VT100*color14: Cyan
! XTerm*VT100*color15: White

XTerm*translations: #override \
  Ctrl <Key> minus: smaller-vt-font() \n\
  Ctrl <Key> plus: larger-vt-font() \n\
  Ctrl <Key> 0: set-vt-font(d)
