## -*- conf -*-

"xbindkeys_show"
  control+shift + q

## audio controls
# "pactl set-sink-volume @DEFAULT_SINK@ +1000"
"volumectl up"
  XF86AudioRaiseVolume

# "pactl set-sink-volume @DEFAULT_SINK@ -1000"
"volumectl down"
  XF86AudioLowerVolume

# "pactl set-sink-mute @DEFAULT_SINK@ toggle"
"volumectl toggle"
  XF86AudioMute

## lock screen
"slock"
  Scroll_Lock

## clipboard manager
"clipmenu"
  Mod4 + y

## pass
"passmenu"
  Mod4 + p
